{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run nuxt build",
    "dev": "bun run nuxt dev",
    "generate": "bun run nuxt generate",
    "preview": "bun run preview:cloudflare",
    "preview:cloudflare": "bun run build && wrangler dev",
    "preview:nuxt": "bun run nuxt preview",
    "postinstall": "bun run nuxt prepare",
    "deploy": "bun run build && bun run wrangler deploy",
    "cf-typegen": "bun run wrangler types",
    "types": "bun run cf-typegen",
    "packages": "bun run rimraf node_modules bun.lock && bun install",
    "clean": "bun run rimraf .nuxt *.d.ts"
  },
  "dependencies": {
    "@nuxt/content": "3.5.1",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.13.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "0.11.7",
    "@nuxt/test-utils": "3.19.1",
    "@nuxt/ui": "3.1.3",
    "@unhead/vue": "^2.0.3",
    "eslint": "^9.0.0",
    "nuxt": "^3.17.4",
    "typescript": "^5.6.3",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.5",
    "@biomejs/biome": "^1.9.4",
    "@trunkio/launcher": "^1.3.4",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.11.12",
    "rimraf": "^6.0.1",
    "wrangler": "^4.17.0"
  },
  "trustedDependencies": [
    "@anthropic-ai/claude-code",
    "@biomejs/biome",
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "better-sqlite3",
    "esbuild",
    "sharp",
    "unrs-resolver",
    "vue-demi",
    "workerd"
  ]
}
