{
  "name": "dave-io-api",
  "version": "0.1.0",
  "private": true,
  "description": "multipurpose personal API",
  "repository": "https://github.com/daveio/dave-io-api.git",
  "license": "MIT",
  "author": "Dave Williams <dave@dave.io>",
  "type": "module",
  "scripts": {
    "build": "bun run clean && bun run types && bun run vite build && bun run wrangler build",
    "check": "bun run build && bun run format && bun run lint && bun run typecheck",
    "clean": "bun run rimraf dist/ auto-imports.d.ts components.d.ts env.d.ts tsconfig.tsbuildinfo worker-configuration.d.ts && find src -type f \\( -name '*.d.ts' -o -name '*.js' -o -name '*.js.map' \\) -delete",
    "types": "bun run wrangler types && bun -e \"Bun.write('worker-configuration.d.ts', '//@ts-nocheck\\n' + await Bun.file('worker-configuration.d.ts').text())\"",
    "deploy": "bun run build && bun run wrangler deploy",
    "dev": "bun run build && bun run wrangler dev",
    "typecheck": "bun run vue-tsc --build --noEmit && bun run tsc --noEmit",
    "lint": "bun run biome check && bun run trunk check -a",
    "fmt": "bun run biome format --write; bun run trunk fmt -a",
    "format": "bun run fmt",
    "kv": "bun run bin/kv.ts",
    "jwt": "bun run bin/jwt.ts",
    "export:kv": "bun scripts/export-kv-keys.ts"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.2.2",
    "@cloudflare/d1": "^1.4.1",
    "@tailwindcss/postcss": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "chanfana": "^2.8.0",
    "cloudflare": "^4.3.0",
    "commander": "^14.0.0",
    "hono": "^4.7.11",
    "ip-address": "^10.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "pinia": "^3.0.3",
    "postcss": "^8.5.4",
    "readline-sync": "^1.4.10",
    "rss-parser": "^3.13.0",
    "tailwindcss": "^4.1.8",
    "uuid": "^11.1.0",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.11",
    "@biomejs/biome": "^1.9.4",
    "@iconify/json": "^2.2.346",
    "@openai/codex": "^0.1.2505172129",
    "@pinegrow/vite-plugin": "^3.0.69",
    "@trunkio/launcher": "^1.3.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/bun": "^1.2.15",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.29",
    "@types/readline-sync": "^1.4.8",
    "@types/uuid": "^10.0.0",
    "@unocss/preset-icons": "^66.1.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "bun": "^1.2.15",
    "markdownlint-cli": "^0.45.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "unocss": "^66.1.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.19.1"
  },
  "packageManager": "bun@1.2.15",
  "engines": {
    "bun": "~1.2.15",
    "node": "~lts"
  },
  "trustedDependencies": [
    "@anthropic-ai/claude-code",
    "@biomejs/biome",
    "@tailwindcss/oxide",
    "bun",
    "esbuild",
    "sharp",
    "workerd"
  ]
}
