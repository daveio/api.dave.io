{
  "name": "api.dave.io",
  "version": "0.1.0",
  "private": true,
  "description": "multipurpose personal API",
  "repository": "https://github.com/daveio/api.dave.io.git",
  "license": "MIT",
  "author": "Dave Williams <dave@dave.io>",
  "type": "module",
  "scripts": {
    "cf-typegen": "bun run wrangler types && bun -e \"Bun.write('worker-configuration.d.ts', '//@ts-nocheck\\n' + await Bun.file('worker-configuration.d.ts').text())\"",
    "types": "bun run cf-typegen",
    "deploy": "bun run wrangler deploy",
    "dev": "bun run wrangler dev",
    "start": "bun run wrangler dev",
    "typecheck": "bun run tsc --noEmit",
    "lint": "bun run biome check; bun run trunk check -a",
    "fmt": "biome format; bun run trunk fmt -a",
    "kv": "bun bin/kv.ts",
    "jwt": "bun bin/jwt.ts"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.2.2",
    "bun": "^1.2.14",
    "chanfana": "2.8.0",
    "commander": "^14.0.0",
    "hono": "^4.7.10",
    "ip-address": "^10.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "readline-sync": "^1.4.10",
    "rss-parser": "^3.13.0",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@trunkio/launcher": "^1.3.4",
    "@types/bun": "^1.2.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.21",
    "@types/readline-sync": "^1.4.8",
    "markdownlint-cli": "^0.45.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.17.0"
  },
  "packageManager": "bun@1.2.14",
  "engines": {
    "bun": "~1.2.14",
    "node": "~lts"
  },
  "trustedDependencies": ["@biomejs/biome", "bun", "esbuild", "sharp", "workerd"]
}
