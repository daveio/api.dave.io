{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "next-dave-io",
  "compatibility_date": "2025-05-29",
  "compatibility_flags": ["nodejs_compat", "nodejs_compat_populate_process_env"],
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "ai": {
    "binding": "AI"
  },
  "kv_namespaces": [
    {
      "binding": "DATA",
      "id": "184eca13ac05485d96de48c436a6f5e6"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "next-api-auth-metadata",
      "database_id": "106894e2-1f5c-4979-a777-0b45febbb993"
    }
  ],
  "r2_buckets": [
    {
      "binding": "IMAGES",
      "bucket_name": "images-dave-io"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "DASHBOARD_WS",
        "class_name": "DashboardWebSocket"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["DashboardWebSocket"]
    }
  ],
  "routes": [
    {
      "pattern": "next.dave.io",
      "custom_domain": true
    }
  ],
  "vars": {
    "CLOUDFLARE_ACCOUNT_ID": "def50674a738cee409235f71819973cf"
  }
}
